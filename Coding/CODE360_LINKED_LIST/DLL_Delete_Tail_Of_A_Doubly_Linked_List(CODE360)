CODE360: Delete Tail of a Doubly Linked List (DLL)

Node * deleteTail(Node *head) {
    if(head==NULL || head->next==NULL) return nullptr;

    Node * temp=head;
    while(temp->next)
    { 
     temp=temp->next;
    }
   Node * prev;
  prev=temp->next;
  prev->next=nullptr;
  temp->back=nullptr;
  delete(temp);
 return head;
 
}
